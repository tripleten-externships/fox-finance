{
  "compilerOptions": {
    "types": ["node"],

    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    
  },
  "include": ["apps", "packages", "prisma.config.ts", "prisma/**/*.prisma", "apps/**/*"]
}
